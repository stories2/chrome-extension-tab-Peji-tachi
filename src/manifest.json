{
  "manifest_version": 3,
  "name": "Peji Tachi",
  "description": "Manage tab branches",
  "version": "0.0.1",
  "author": "stories2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Open template popup"
  }
}
